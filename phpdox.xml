<?xml version="1.0" encoding="UTF-8"?>
<phpdox xmlns="http://xml.phpdox.net/config">
  <project name="collapsing-category-list" source="wp-content/plugins/collapsing-category-list" workdir="build/phpdox">
    <collector publiconly="false">
      <include mask="*.php" />
    </collector>
    
    <generator output="build">
      <build engine="html" enabled="true" output="api">
        <file extension="html" />
      </build>
    </generator>
  </project>
</phpdox>
